[global_config]
  enabled_plugins = CustomCommandsMenu, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  title_hide_sizetext = True
  title_transmit_bg_color = "#03c800"
[keybindings]
  close_term = <Primary>w
  copy = <Primary>c
  cycle_next = <Primary><Alt>Tab
  cycle_prev = <Primary><Shift><Alt>Tab
  new_tab = <Primary>t
  next_tab = <Primary>Tab
  paste = <Primary>v
  prev_tab = <Primary><Shift>Tab
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
  [[EditorPlugin]]
    command = gvim --remote-silent +{line} {filepath}
    groups = file line
    match = ([^ \t\n\r\f\v:]+?):([0-9]+)
[profiles]
  [[default]]
    background_image = None
    cursor_color = "#aaaaaa"
